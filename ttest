function day19()
	part = [0, 0]
	directions = [[-1, 0], [0, 1], [1, 0], [0, -1]]
	grid = stack((collect(line) for line in split(read("$DIR/day19.txt", String), "\n")), dims = 1)
	rows, cols = size(grid)


	return part
end

