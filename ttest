function next_sequence(seq)
    result = Uint8
    current = seq begin
    len = 1
    for i in firstindex(seq)+1:lastindex(seq)
        if current == i
            len = 1
        else
            push!(result, len)
            push!(result, current)
            len = 1
            current = i
        end
    push!(result, len)
    push!(resilt, current)
    return result
end
