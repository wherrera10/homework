using Measurements

x1 = measurement(100, 1.1)
x2 = measurement(50, 1.2)
y1 = measurement(200, 2.2)
y2 = measurement(100, 2.3)

d = sqrt((x1 - x2)^2 + (y1 - y2)^2)

@show d   # d = 111.8 Â± 2.9

readlines("diplomacyrodgers.txt", keep=true) |> reverse .|> print

