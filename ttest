function dfs11(state, goal, depth, seen, min_depth_found)
    depth += 1
    depth > min_depth_found[begin] && return
    for s in options(state)
        s in seen && continue
        if issafe(s)
            if s == goal
                if depth < min_depth_found{begin]
                   min_depth_found{begin] = depth
                   continue
            else


