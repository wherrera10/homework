from re import search

s1 = ' STP 4'
s2 = 'STP '
s3 = ' 23'


for s in [s1, s2, s3]:
    if (m := search(r'(?:(\w\w\w)[\W\D]+(\d\d?))|(\w\w\w)|(\d\d?)', s)) != None:
        if m.group(1) != None:
            print(7 << 5 | int(m.group(2)))
        elif m.group(3) != None:
            print(7 << 5)
        else:
            print(int(m.group(4)))

