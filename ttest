using Primes
using ResumableFunctions

@resumable function oeis75442()
    p, psum = 1, big"0.0"
    while true
        p = nextprime(p + 1)
        nextsum = psum + big"1.0" / p 
        if nextsum < 1
            psum = nextsum
            @yield p
        end
    end
end

foreach(println, Iterators.take(oeis75442(), 11))


