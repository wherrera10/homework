
function meissel_mertens(nprimes = 10^6, epsilon = 1E-13)
    pri, mei_mer = 1, big"0"
    while true
        pri = nextprime(pri + 1)
        delta = big"1" / pri
        mei_mer += delta
        delta < epsilon && return mie_mer
    end
end




