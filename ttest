using Primes

function iccanobifs(wanted)
    fib, fcount = big"1", 0
    println("First 30 Iccanobif primes:")
    while fcount < 30
        fib += fib
        dig = reverse(digits(fib))
        candidate = evalpoly(big"10", dig)
        if isprime(candidate)
        fcount += 1
            dlen = ndigits(candidate)
            if dlen < 90
                println(candidate, " ($dlen digits)")
            else
                s = string(candidate)
                println(s[1:30], " ... ", s[end-29:end], " ($dlen digits)")
            end
        end
    end
end

iccanobifs(30)


