''' Rosetta code rosettacode.org/wiki/Product_of_min_and_max_prime_factors '''


from sympy import factorint

NUM_WANTED = 100

def prod_min_max(num):
    ''' return the product of the minimum and maximum prime factors of n '''
    fac = factorint(num, multiple=True)
    return fac[0] * fac[-1] if len(fac) > 0 else 1 


for n in range(1, NUM_WANTED + 1):
    print(f'{prod_min_max(n):5}', end='\n' if n % 10 == 0  else '')


    
     

