function jp(limit)
    res = map(factorial, UInt64(1):18)
    k = 2
    while k < length(res)
        rk = res[k]
        for j = 2:length(res)
            kl = res[j] * rk
            kl > limit && break
            while kl <= limit && kl âˆ‰ res
                push!(res, kl)
                kl *= rk
             end
        end
        k += 1
    end
    return sort!(Int.(res))
end

@show jp(2^53-1)
@time jp(2^53-1)

