function unescape_day08(s)
    s = replace(s, "\\"" => "\"")
    s = replace(s, "\\\\" => "\\")
    return replace(s, r"\\x[0123456789abcdef]{2}" => x -> Char(parse(Int32, x[3:4], base=16)))
end 
function day08()
    part = [0, 0]
    lines = readlines("$DIR/day08.txt")
    newlines = map(unescape_day08, lines)
    part[1] = sum(length, lines) - sum(x -> length(process_day08(x)), lines)

    return part
end

