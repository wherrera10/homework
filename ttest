function next_sequence10(seq)
    result = Int32[]
    current = seq[begin]
    len = 1
    for i in @view seq[2:end]
        if current == i
            len += 1
        else
            push!(result, len)
            push!(result, current)
            len = 1
            current = i
        end
    end
    push!(result, len)
    push!(result, current)
    return result
end




    
